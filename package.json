{
  "name": "nuxt-app",
  "version": "1.0.0",
  "description": "My stylish Nuxt.js project",
  "author": "Yuta Haga",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint": "npm run lint:vue && npm run lint:css",
    "lint:vue": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint \"src/**/*.{css,vue}\"",
    "format": "npm run format:vue && npm run format:css",
    "format:vue": "eslint --fix --ext .ts,.vue --ignore-path .gitignore .",
    "format:css": "stylelint \"src/**/*.{css,vue}\" --fix",
    "format:css:watch": "stylelint-watch-autofix css \"src/**/*.{css,vue}\"",
    "precommit": "npm run lint",
    "tsc": "tsc --showConfig",
    "testc": "tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "browserslist": [
    ">0.5%",
    "last 2 versions",
    "ie >= 11",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.6.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.23",
    "@types/webpack": "^4.4.24",
    "nuxt-ts": "^2.4.3",
    "object-fit-images": "^3.2.4",
    "tailwindcss": "^0.7.4",
    "tailwindcss-object-fit": "^1.0.1",
    "vue-hooks": "^0.3.2",
    "vue-property-decorator": "^7.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-import-order-alphabetical": "0.0.1",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.1.0",
    "jest": "^24.1.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-graphql": "^2.1.0",
    "nuxt-purgecss": "^0.2.1",
    "postcss-calc": "^7.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-nested": "^4.1.1",
    "postcss-object-fit-images": "^1.1.2",
    "prettier": "^1.16.4",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^9.10.1",
    "stylelint-config-recess-order": "^2.0.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-declaration-strict-value": "^1.1.2",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylelint-watch-autofix": "github:yutahaga/stylelint-watch-autofix#master",
    "tailwindcss-aspect-ratio": "^1.0.1",
    "ts-jest": "^23.10.5",
    "vue-jest": "^3.0.2"
  }
}
